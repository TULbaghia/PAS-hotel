<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.3" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd">
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <!-- >>>USER -->
        <navigation-case>
            <from-outcome>ListAllUser</from-outcome>
            <to-view-id>/user/ListAllUser.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <!-- >>>GUEST -->
        <navigation-case>
            <from-outcome>ListAllGuest</from-outcome>
            <to-view-id>/user/guest/ListAllGuest.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>AddGuest</from-outcome>
            <to-view-id>/user/guest/AddGuest.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>EditGuest</from-outcome>
            <to-view-id>/user/guest/EditGuest.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>DetailGuest</from-outcome>
            <to-view-id>/user/guest/DetailGuest.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <!-- <<<GUEST -->
        <!-- >>>MANAGER -->
        <navigation-case>
            <from-outcome>ListAllManager</from-outcome>
            <to-view-id>/user/manager/ListAllManager.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>AddManager</from-outcome>
            <to-view-id>/user/manager/AddManager.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>EditManager</from-outcome>
            <to-view-id>/user/manager/EditManager.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <!-- <<<MANAGER -->
        <!-- >>>ADMIN -->
        <navigation-case>
            <from-outcome>ListAllAdmin</from-outcome>
            <to-view-id>/user/admin/ListAllAdmin.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>AddAdmin</from-outcome>
            <to-view-id>/user/admin/AddAdmin.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>EditAdmin</from-outcome>
            <to-view-id>/user/admin/EditAdmin.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <!-- <<<ADMIN -->
        <!-- <<<USER -->

        <!-- >>>APARTMENT -->
        <navigation-case>
            <from-outcome>ListAllApartment</from-outcome>
            <to-view-id>/apartment/ListAllApartment.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>DetailApartment</from-outcome>
            <to-view-id>/apartment/DetailApartment.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <!-- >>>THREE STAR -->
        <navigation-case>
            <from-outcome>ListAllThreeStar</from-outcome>
            <to-view-id>/apartment/threestar/ListAllThreeStar.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>AddThreeStar</from-outcome>
            <to-view-id>/apartment/threestar/AddThreeStar.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>EditThreeStar</from-outcome>
            <to-view-id>/apartment/threestar/EditThreeStar.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <!-- <<<THREE STAR -->
        <!-- >>>FIVE STAR -->
        <navigation-case>
            <from-outcome>ListAllFiveStar</from-outcome>
            <to-view-id>/apartment/fivestar/ListAllFiveStar.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>AddFiveStar</from-outcome>
            <to-view-id>/apartment/fivestar/AddFiveStar.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>EditFiveStar</from-outcome>
            <to-view-id>/apartment/fivestar/EditFiveStar.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <!-- <<<FIVE STAR -->
        <!-- <<<APARTMENT -->

        <!-- >>>RESERVATION -->
        <navigation-case>
            <from-outcome>ListAllReservation</from-outcome>
            <to-view-id>/reservation/ListAllReservation.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>AddReservation</from-outcome>
            <to-view-id>/reservation/AddReservation.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>EditReservation</from-outcome>
            <to-view-id>/reservation/EditReservation.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <!-- <<<RESERVATION -->
        <navigation-case>
            <from-outcome>Index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <application>
        <message-bundle>messages</message-bundle>
        <resource-bundle>
            <base-name>messages</base-name>
            <var>msg</var>
        </resource-bundle>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>en</supported-locale>
            <supported-locale>pl</supported-locale>
        </locale-config>
    </application>
</faces-config>